{
    "name":"用户",
    "desc":"用户的登录、退出、密码修改、获取用户当前登录是否有效，以及更新用户在线状态",
    "api":
    [{
        "name":"用户登录",
        "desc":"",
        "authorization":true,
        "url":"/login/",
        "method": "POST",
        "body_mode":"json",
        "body":{
            "username":{"name":"用户名", "desc":"", "type":"string", "max_length":30},
            "password":{"name":"用户登录密码", "desc":"", "type":"string", "max_length":30, "min_length":3}
        },
        "response":{
            "code":{"$ref":"./_data/models.json:Response.code", "enum":[[-4, "用户名未填写"], [-3, "用户名不存在"], [-2, "密码未填写"], [-1, "密码输入不正确"], [1, "登录成功"]]},
            "msg":{"name":"登录成功返回消息", "type":"string", "desc":"通常返回都是空"},
            "token":{"name":"登录成功返回的用户token", "type":"string", "length":43}
        },
        "test_data":[
            {
                "request":{"username":"edison", "password":"123"},
                "response":{"code":-1, "msg":"密码输入不正确"}
            },
            {
                "request":{"username":"lily", "password":"123"},
                "response":{"code":-3, "msg":"用户名不存在"}
            },
            {
                "request":{"username":"root", "password":"123"},
                "response":{"code":1, "msg":"登录成功", "token":"fjdlkfjlafjdlaj3jk2l4j"}
            },
            {
                "request":{"username":"lily"},
                "response":{"code":-2, "msg":"密码是必填的"}
            },
            {
                "request":{"password":"123"},
                "response":{"code":-4, "msg":"用户名是必填的"}
            },
            {
                "response":{"code":-4, "msg":"用户名是必填的999"}
            }
        ]
    },
    {
        "name":"用户退出登录",
        "desc":"",
        "url":"/logout/",
        "method":"GET",
        "response":{
            "code":{"$ref":"./_data/models.json:Response.code"},
            "msg":{"name":"退出操作返回消息", "desc":"通常返回都是空", "type":"string"}
        },
        "test_data":[
            {
                "response":{"code":1, "msg":"退出成功"}
            }
        ]
    },
    {
        "name":"修改密码",
        "desc":"",
        "url":"/auth/change_password/",
        "method":"POST",
        "body_mode":"json",
        "body":{
            "old_password":{"name":"原始密码", "type":"string", "max_length":30},
            "new_password":{"name":"新密码", "type":"string", "max_length":30, "min_length":3},
            "new_password_confirm":{"name":"重复新密码", "type":"string", "max_length":30, "min_length":3}
        },
        "response":{
            "code":{"$ref":"./_data/models.json:Response.code"},
            "msg":{"name":"退出操作返回消息", "desc":"通常返回都是空", "type":"string"}
        },
        "test_data":[
            {
                "request":{"old_password":"123", "new_password":"1234", "new_password_confirm":"1234"},
                "response":{"code":1, "msg":"密码修改成功"}
            },
            {
                "request":{"old_password":"111", "new_password":"1234", "new_password_confirm":"1234"},
                "response":{"code":-1, "msg":"原始密码输入不正确"}
            },
            {
                "request":{"old_password":"1234", "new_password":"1235", "new_password_confirm":"1234"},
                "response":{"code":-1, "msg":"两次新密码输入不一致"}
            },
            {
                "request":{"new_password":"1235", "new_password_confirm":"1234"},
                "response":{"code":-1, "msg":"请输入原始密码"}
            },
            {
                "request":{"old_password":"1234", "new_password":"1235"},
                "response":{"code":-1, "msg":"两次新密码输入不一致"}
            },
            {
                "request":{"old_password":"1234", "new_password_confirm":"1235"},
                "response":{"code":-1, "msg":"两次新密码输入不一致"}
            },
            {
                "request":{"old_password":"1234"},
                "response":{"code":-1, "msg":"请输入新密码"}
            }
        ]
    }
]}